{
  "name": "language-latex2e",
  "version": "0.13.6",
  "description": "A smarter grammar scoper for LaTeX",
  "main": "./lib/language-latex2e.js",
  "repository": "https://github.com/Aerijo/language-latex2e",
  "bugs": "https://github.com/Aerijo/language-latex2e/issues",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "keywords": [
    "latex",
    "syntax",
    "highlighting",
    "grammar"
  ],
  "coffeelintConfig": {
    "max_line_length": {
      "level": "ignore"
    },
    "indentation": {
      "level": "error",
      "value": 2
    }
  }
}
